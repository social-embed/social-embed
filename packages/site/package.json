{
  "name": "@social-embed/site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "clean": "yarn clear",
    "ncu": "ncu",
    "update": "ncu",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "prebuild": "yarn docs:oembed",
    "prestart": "yarn docs:oembed",
    "docs:oembed": "yarn workspace @social-embed/wc build && mkdir -p static/js && cp ../wc/dist/OEmbedElement.umd.cjs static/js/o-embed.bundled.js",
    "lint": "biome lint .",
    "biome": "biome",
    "format": "yarn run biome format . --write",
    "test": "echo 'NOOP'"
  },
  "dependencies": {
    "@docusaurus/core": "3.4.0",
    "@docusaurus/preset-classic": "3.4.0",
    "@docusaurus/theme-classic": "^3.4.0",
    "@docusaurus/theme-live-codeblock": "^3.4.0",
    "@mdx-js/react": "^3.0.1",
    "clsx": "^2.1.1",
    "prism-react-renderer": "^2.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@docusaurus/module-type-aliases": "3.4.0",
    "@docusaurus/tsconfig": "^3.4.0",
    "@docusaurus/types": "3.4.0",
    "typescript": "^5.5.4"
  },
  "browserslist": {
    "production": [">0.5%", "not dead", "not op_mini all"],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18"
  }
}
