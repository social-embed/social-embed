{
  "name": "@social-embed/wc",
  "version": "0.0.1-next.6",
  "description": "A drop-in component for <o-embed> tags",
  "main": "./dist/o-embed.js",
  "module": "./dist/o-embed.js",
  "types": "./dist/o-embed.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist/",
    "ncu": "ncu",
    "update": "ncu",
    "lint": "npm run lint:lit-analyzer && npm run lint:eslint",
    "lint:eslint": "eslint 'src/**/*.ts'",
    "lint:lit-analyzer": "lit-analyzer -- --rules.no-boolean-in-attribute-binding off --rules.no-incompatible-type-binding off",
    "format": "prettier src/* --write",
    "analyze": "wca analyze \"src/**/*.ts\" --outFile ../site/custom-elements.json",
    "serve": "es-dev-server --node-resolve --watch",
    "test": "wtr dist/test/o-embed_test.js --puppeteer --node-resolve",
    "test:watch": "yarn test --watch",
    "test:update-snapshots": "karma start karma.conf.cjs --update-snapshots",
    "test:prune-snapshots": "karma start karma.conf.cjs --prune-snapshots",
    "checksize": "rollup -c ; cat dist/o-embed.bundled.js | gzip -9 | wc -c ; rm dist/o-embed.bundled.js",
    "wtr": "wtr"
  },
  "keywords": [
    "web-component",
    "web-components",
    "lit-element",
    "typescript",
    "oembed",
    "o-embed"
  ],
  "author": "Tony Narlock <tony@git-pull.com> (https://www.git-pull.com)",
  "license": "MIT",
  "homepage": "https://social-embed.git-pull.com/",
  "bugs": {
    "url": "https://github.com/social-embed/social-embed/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/social-embed/social-embed"
  },
  "dependencies": {
    "@social-embed/lib": "^0.0.1-next.10",
    "lit-element": "^2.4.0"
  },
  "devDependencies": {
    "@open-wc/karma-esm": "^4.0.0",
    "@open-wc/testing": "^2.5.32",
    "@open-wc/testing-karma": "^4.0.9",
    "@rollup/plugin-replace": "^2.4.2",
    "@types/chai": "^4.2.15",
    "@types/mocha": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "@web/test-runner": "^0.12.17",
    "@web/test-runner-puppeteer": "^0.9.3",
    "chai": "^4.3.4",
    "deepmerge": "^4.2.2",
    "eslint": "^7.23.0",
    "karma": "^6.3.2",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^2.0.1",
    "lit-analyzer": "^1.2.1",
    "mocha": "^8.3.2",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.44.0",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.2.3",
    "web-component-analyzer": "^1.1.6"
  }
}
